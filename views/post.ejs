<%- include("header") -%>
    <h1>
        <%= obj.id %>
    </h1>
    <h2>
        <%= obj.blogname %>
    </h2>
    <p>
        <%= obj.description %>
    </p>


    <form method="POST">
        <!-- <input type="text" name="id" > -->
        <input type="text" value="<%= obj.blogname %>" placeholder="Blog Title" name="post">
        <input type="textarea" value="<%= obj.description %>" name="postdesc" placeholder="Description">
        <button type="submit">submit</button>
    </form>
    <%- include("footer") -%>